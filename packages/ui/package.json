    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.1",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-vue": "^10.0.0",
    "happy-dom": "^17.4.4",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "sass": "^1.87.0",
    "storybook": "8.6.12",
